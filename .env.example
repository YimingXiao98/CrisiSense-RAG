# OPENAI_API_KEY -> only needed when MODEL_PROVIDER=openai
OPENAI_API_KEY=
# GEMINI_API_KEY -> required when MODEL_PROVIDER=gemini
GEMINI_API_KEY=
# GEMINI_MODEL -> optional override (default models/gemini-1.5-flash)
GEMINI_MODEL=models/gemini-1.5-flash
# GEMINI_MAX_ATTEMPTS -> retry budget for provider calls
GEMINI_MAX_ATTEMPTS=3

# MODEL_PROVIDER options: mock | openai | gemini
MODEL_PROVIDER=mock

# DEFAULT_TIME_WINDOW_DAYS is used when chat messages omit dates
DEFAULT_TIME_WINDOW_DAYS=7

# DATA_DIR points to the repo data folder
DATA_DIR=./data

HARVEY_ENABLE_RERANKER=1
